{
  "name": "lasereyes-mono",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:core": "turbo build --filter @omnisat/lasereyes-core",
    "build:react": "turbo build --filter @omnisat/lasereyes-react",
    "build:lasereyes": "turbo build --filter @omnisat/lasereyes",
    "dev": "turbo dev",
    "dev:demo": "turbo dev --filter demo.lasereyes.build",
    "dev:docs": "turbo dev --filter documentation-site",
    "dev:react": "turbo dev --filter react-ui",
    "dev:vue": "turbo dev --filter vue-ui",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "publish": "echo 'No publish script found'"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/jest": "^29.5.13",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
